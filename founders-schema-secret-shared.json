{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Founder Personality Traits",
  "type": "object",
  "properties": {
    "fullName": {
      "type": "string",
      "description": "The full name of the founder"
    },
    "occupation": {
      "type": "string",
      "description": "The occupation or professional role of the founder"
    },
    "country": {
      "type": "string",
      "description": "The country where the founder is based"
    },
    "region": {
      "type": "string",
      "description": "The specific region or state within the country where the founder is based"
    },
    "traits": {
      "type": "object",
      "description": "Key personality traits of the founder",
      "properties": {
        "visionary": {
          "type": "object",
          "properties": {
            "value": {
              "type": "object",
              "properties": {
                "$share": {
                  "type": "string",
                  "description": "Indicates if the founder has a forward-thinking mindset, able to foresee future trends and opportunities in their field."
                }
              },
              "required": [
                "$share"
              ]
            },
            "explanation": {
              "type": "string",
              "description": "Explanation for why the founder is considered visionary or not."
            }
          },
          "required": [
            "value",
            "explanation"
          ]
        },
        "resilience": {
          "type": "object",
          "properties": {
            "value": {
              "type": "object",
              "properties": {
                "$share": {
                  "type": "string",
                  "description": "The ability to overcome obstacles and persist despite challenges, rated on a scale of 1 to 10."
                }
              },
              "required": [
                "$share"
              ]
            },
            "explanation": {
              "type": "string",
              "description": "Explanation for why this level of resilience was assigned."
            }
          },
          "required": [
            "value",
            "explanation"
          ]
        },
        "riskTolerance": {
          "type": "object",
          "properties": {
            "value": {
              "type": "object",
              "properties": {
                "$share": {
                  "type": "string",
                  "description": "The founder’s willingness to take calculated risks, important for innovation and growth, rated on a scale of 1 to 10."
                }
              },
              "required": [
                "$share"
              ]
            },
            "explanation": {
              "type": "string",
              "description": "Explanation for why this level of risk tolerance was assigned."
            }
          },
          "required": [
            "value",
            "explanation"
          ]
        },
        "adaptability": {
          "type": "object",
          "properties": {
            "value": {
              "type": "object",
              "properties": {
                "$share": {
                  "type": "string",
                  "description": "The ability to quickly adjust to changing circumstances and pivot strategies when necessary, rated on a scale of 1 to 10."
                }
              },
              "required": [
                "$share"
              ]
            },
            "explanation": {
              "type": "string",
              "description": "Explanation for why this level of adaptability was assigned."
            }
          },
          "required": [
            "value",
            "explanation"
          ]
        },
        "charisma": {
          "type": "object",
          "properties": {
            "value": {
              "type": "object",
              "properties": {
                "$share": {
                  "type": "string",
                  "description": "The founder’s charm and persuasive power, helpful for inspiring teams and attracting stakeholders, rated on a scale of 1 to 10."
                }
              },
              "required": [
                "$share"
              ]
            },
            "explanation": {
              "type": "string",
              "description": "Explanation for why this level of charisma was assigned."
            }
          },
          "required": [
            "value",
            "explanation"
          ]
        },
        "confidence": {
          "type": "object",
          "properties": {
            "value": {
              "type": "object",
              "properties": {
                "$share": {
                  "type": "string",
                  "description": "The level of self-assurance and conviction in their vision and abilities, rated on a scale of 1 to 10."
                }
              },
              "required": [
                "$share"
              ]
            },
            "explanation": {
              "type": "string",
              "description": "Explanation for why this level of confidence was assigned."
            }
          },
          "required": [
            "value",
            "explanation"
          ]
        },
        "drive": {
          "type": "object",
          "properties": {
            "value": {
              "type": "object",
              "properties": {
                "$share": {
                  "type": "string",
                  "description": "The founder’s motivation and determination to achieve their goals, rated on a scale of 1 to 10."
                }
              },
              "required": [
                "$share"
              ]
            },
            "explanation": {
              "type": "string",
              "description": "Explanation for why this level of drive was assigned."
            }
          },
          "required": [
            "value",
            "explanation"
          ]
        },
        "communicationSkills": {
          "type": "object",
          "properties": {
            "value": {
              "type": "object",
              "properties": {
                "$share": {
                  "type": "string",
                  "description": "The founder’s ability to convey ideas effectively to various audiences, rated on a scale of 1 to 10."
                }
              },
              "required": [
                "$share"
              ]
            },
            "explanation": {
              "type": "string",
              "description": "Explanation for why this level of communication skills was assigned."
            }
          },
          "required": [
            "value",
            "explanation"
          ]
        },
        "empathy": {
          "type": "object",
          "properties": {
            "value": {
              "type": "object",
              "properties": {
                "$share": {
                  "type": "string",
                  "description": "The ability to understand and connect with others’ emotions, essential for team building and customer relations, rated on a scale of 1 to 10."
                }
              },
              "required": [
                "$share"
              ]
            },
            "explanation": {
              "type": "string",
              "description": "Explanation for why this level of empathy was assigned."
            }
          },
          "required": [
            "value",
            "explanation"
          ]
        },
        "decisionMaking": {
          "type": "object",
          "properties": {
            "value": {
              "type": "object",
              "properties": {
                "$share": {
                  "type": "string",
                  "description": "The founder’s capability to make prompt, effective decisions, balancing analysis with intuition, rated on a scale of 1 to 10."
                }
              },
              "required": [
                "$share"
              ]
            },
            "explanation": {
              "type": "string",
              "description": "Explanation for why this level of decision-making was assigned."
            }
          },
          "required": [
            "value",
            "explanation"
          ]
        }
      },
      "required": [
        "visionary",
        "resilience",
        "riskTolerance",
        "adaptability",
        "drive",
        "decisionMaking"
      ]
    }
  },
  "required": [
    "fullName",
    "occupation",
    "country",
    "region",
    "traits"
  ]
}
